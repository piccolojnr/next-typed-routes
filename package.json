{
  "name": "next-typed-routes",
  "version": "0.1.0",
  "description": "Type-safe route generation and navigation for Next.js App Router",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./route-helper": {
      "types": "./dist/route-helper.d.ts",
      "import": "./dist/route-helper.mjs",
      "require": "./dist/route-helper.js"
    }
  },
  "bin": {
    "next-typed-routes": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "generate": "node dist/cli.js",
    "generate:watch": "node dist/cli.js --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "routes",
    "type-safe",
    "navigation",
    "app-router",
    "generator",
    "cli"
  ],
  "author": "Piccolojnr <daudrahim566@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/piccolojnr/next-typed-routes.git"
  },
  "bugs": {
    "url": "https://github.com/piccolojnr/next-typed-routes/issues"
  },
  "homepage": "https://github.com/piccolojnr/next-typed-routes#readme",
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "chokidar": "^3.5.3"
  }
}